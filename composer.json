{
  "name": "vaadah/laravel-datatables",
  "description": "jQuery DataTables API for Laravel 4|5",
  "keywords": [
    "laravel",
    "dataTables",
    "jquery"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "srikanth",
      "email": "srikanth.240369@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": "5.4.*|5.5.*|5.6.*",
    "illuminate/filesystem": "5.4.*|5.5.*|5.6.*",
    "illuminate/http": "5.4.*|5.5.*|5.6.*",
    "illuminate/support": "5.4.*|5.5.*|5.6.*",
    "illuminate/view": "5.4.*|5.5.*|5.6.*"
  },
  "require-dev": {
    "orchestra/testbench": "~3.5"
  },
  "suggest": {
    "Vaadah/laravel-datatables-buttons": "Plugin for server-side exporting of dataTable.",
    "Vaadah/laravel-datatables-html": "Plugin for server-side HTML builder of dataTable.",
    "Vaadah/laravel-datatables-fractal": "Plugin for server-side response using Fractal.",
    "Vaadah/laravel-datatables-editor": "Plugin to use Datatables Editor (requires a license)."
  },
  "autoload": {
    "psr-4": {
      "Vaadah\\DataTables\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Vaadah\\DataTables\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.0-dev"
    },
    "laravel": {
      "providers": [
        "Vaadah\\DataTables\\DataTablesServiceProvider"
      ],
      "aliases": {
        "DataTables": "Vaadah\\DataTables\\Facades\\DataTables"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
